<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainBorderPane" style="-fx-background-color: white" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dzieniu2.controller.employee.EmployeeController">
    <right>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <HBox>
                    <children>
                        <JFXButton fx:id="previousDispenserButton" buttonType="RAISED" mnemonicParsing="false" onAction="#previousDispenser" prefHeight="100.0" prefWidth="110.0" style="-fx-background-color: ffffff" text="&lt;">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></JFXButton>
                        <JFXButton fx:id="dispenserButton" buttonType="RAISED" mnemonicParsing="false" onAction="#openDispenser" prefHeight="100.0" prefWidth="239.0" style="-fx-background-color: ffffff" text="dispenserName">
                            <HBox.margin>
                                <Insets right="20.0" />
                            </HBox.margin></JFXButton>
                        <JFXButton fx:id="nextDispenserButton" buttonType="RAISED" mnemonicParsing="false" onAction="#nextDispenser" prefHeight="100.0" prefWidth="110.0" style="-fx-background-color: ffffff" text="&gt;">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></JFXButton>
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
                <VBox prefHeight="900.0" prefWidth="500.0">
                    <children>
                        <JFXButton buttonType="RAISED" mnemonicParsing="false" onMouseClicked="#openProductList" prefHeight="100.0" prefWidth="500.0" style="-fx-background-color: ffffff" text="Product list">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin></JFXButton>
                        <Separator prefHeight="100.0" prefWidth="500.0" />
                        <JFXButton buttonType="RAISED" mnemonicParsing="false" onMouseClicked="#openTransactionWindow" prefHeight="100.0" prefWidth="500.0" style="-fx-background-color: ffffff" stylesheets="@../../css/Styling.css" text="Transaction">
                            <VBox.margin>
                                <Insets bottom="40.0" />
                            </VBox.margin>
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-1" />
                        <String fx:value="btn-1e" />
                     </styleClass>
                        </JFXButton>
                        <JFXButton buttonType="RAISED" mnemonicParsing="false" onMouseClicked="#openAddClient" prefHeight="100.0" prefWidth="500.0" style="-fx-background-color: ffffff" text="New client" />
                        <BorderPane fx:id="cameraPane" prefHeight="400" prefWidth="500" />
                    </children>
                </VBox>
            </children>
            <BorderPane.margin>
                <Insets left="20.0" right="20.0" />
            </BorderPane.margin>
        </VBox>
    </right>
</BorderPane>

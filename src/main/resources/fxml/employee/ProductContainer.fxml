            <Insets top="20.0" />
         </BorderPane.margin>
         <font>
            <Font size="18.0" />
         </font></Label>
    </top>
    <center>
        <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="73.0" layoutY="73.0" pickOnBounds="true" />
    </center>
    <bottom>
        <HBox alignment="center" prefHeight="100">
            <children>
                <Label fx:id="remainingLabel" alignment="CENTER" prefHeight="40.0" prefWidth="245.0">
                    <HBox.margin>
                        <Insets left="20.0" />
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
    </bottom>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainBorderPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dzieniu2.controller.employee.EmployeeController">
    <right>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <HBox>
                    <children>
                        <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="166.0" text="Station #1" />
                        <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="166.0" text="Station #2" />
                        <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="167.0" text="Station #3" />
                    </children>
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </HBox>
                <VBox prefHeight="900.0" prefWidth="500.0">
                    <children>
                        <Button mnemonicParsing="false" onMouseClicked="#toGasTanks" prefHeight="100.0" prefWidth="500.0" text="Gas Tanks">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </Button>
                        <Separator prefHeight="100.0" prefWidth="500.0" />
                        <Button mnemonicParsing="false" onMouseClicked="#toOne" prefHeight="100.0" prefWidth="500.0" text="One">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onMouseClicked="#toTwo" prefHeight="100.0" prefWidth="500.0" text="Two">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onMouseClicked="#toThree" prefHeight="100.0" prefWidth="500.0" text="Three" />
                        <AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #909090;">
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </AnchorPane>
                    </children>
                </VBox>
            </children>
            <BorderPane.margin>
                <Insets left="20.0" right="20.0" />
            </BorderPane.margin>
        </VBox>
    </right>
</BorderPane>
